/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    // Put your code here:
	Or(a=in[0],b=in[1],out=res1);
	Or(a=in[2],b=in[3],out=res2);
	Or(a=in[4],b=in[5],out=res3);
	Or(a=in[6],b=in[7],out=res4);
	Or(a=res1,b=res2,out=stmt1);
	Or(a=res3,b=res4,out=stmt2);
	Or(a=stmt1,b=stmt2,out=out);
}